<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaeyoungyun.todo2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.LogInUI">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.SignUpUI" />
        <activity android:name=".UI.AddSubjectUI" />
        <activity android:name=".UI.AddTodoUI" />
        <activity android:name=".UI.TodoListUI" />
        <activity android:name=".UI.ModifySubjectUI" />
        <activity android:name=".UI.ModifyTodoUI" />
        <activity android:name=".UI.ScheduleUI" />
        <activity android:name=".UI.CalendarUI" />
        <activity android:name=".UI.CompletedTodoListUI" />
        <activity android:name=".UI.TodoUI" />
        <activity android:name=".UI.SubjectTodoListUI" />
        <activity android:name=".UI.DayTodoListUI" />
        <activity android:name=".UI.HiddenTodoListUI"></activity>
    </application>

</manifest>